<files .htaccess>
order allow,deny
deny from all
</files>

<Files "users.xml">  
  Order Allow,Deny
  Deny from all
</Files>

RewriteEngine On

# Redirection pages
RewriteRule Accueil index.php
RewriteRule Présentation presentation.php
RewriteRule Maison-de-la-Veille-Social mvs.php
RewriteRule Pole-Ingenierie-et-Developpement pid.php
RewriteRule Chantiers-Insertion chantiers.php
RewriteRule Articles articles.php
RewriteRule Article article.php
RewriteRule Contact contact.php
RewriteRule Mentions-légales mentions.php
RewriteRule Politique-de-confidentialité confidentialite.php

# Partie Admin
RewriteRule Administration admin.php

RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} !^localhost [NC]
RewriteCond %{REMOTE_ADDR} !^127\.0\.0\.1$
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]


RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^/]+)/$ $1.php

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !(\.[a-zA-Z0-9]{1,5}|/)$
RewriteRule (.*)$ /$1/ [R=301]

  ## www -> no-www
RewriteCond %{HTTP_HOST} ^www.ancre08.fr$ 
RewriteRule ^ https://ancre08.fr [R=301,L,NE,QSA]